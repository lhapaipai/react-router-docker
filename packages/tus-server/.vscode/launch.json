  "version": "0.2.0",
  "configurations": [

    {
      "name": "Run current ts file with tsx",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "tsx",
      "runtimeArgs": ["${relativeFile}"]
    },
    {
      "name": "Test current ts file with vitest",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "vitest",
      "runtimeArgs": ["${relativeFile}"]
    }
  ]
}
